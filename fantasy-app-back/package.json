{
  "name": "fantasy-app-back",
  "version": "0.0.1",
  "description": "Fantasy app backend",
  "main": "service/server/index.ts",
  "engines": {
    "node": ">=8.15.0",
    "npm": ">=6.4.1"
  },
  "scripts": {
    "start": "node dist/index.js",
