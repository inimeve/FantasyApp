    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/compression": "0.0.35",
    "@types/cookie-parser": "1.4.1",
    "@types/express": "4.11.1",
    "@types/node-fetch": "^2.5.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.9.2",
    "ts-node": "6.1.1",
    "typescript": "3.1.1"
